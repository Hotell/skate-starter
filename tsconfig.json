{
  "compilerOptions": {
    "module": "esnext",
    "target": "es5",
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "jsxFactory": "h",
    "importHelpers": true,
    "pretty": true,
    "outDir": "ts-output",
    "lib": [
      "dom",
      "esnext"
    ],
    "baseUrl": ".",
    "paths": {
      "skatejs": [
        "manual_types/skatejs"
      ],
      "@skatejs/*": [
        "manual_types/@skatejs/*"
      ]
    }
  },
  "include": [
    "src/"
  ]
}
